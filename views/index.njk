{% extends "layout.njk" %}


{% block content %}


<div class="container p-3 my-3  text-white ">
  <div class="row">
    <button type="button" class="btn btn-outline-light shadow-none">
      <a href="{{allPlatformsNameUnique[0]}}">
      <img src="{{urlPlatformsUnique[0]}}">
      </a>
    </button>
  </div>
  <div class="row align-self-end">
  {%for item in myPlatform0 %}
    {% if loop.index0 < 4 %}
    <div class="col align-self-end view overlay zoom" >
      <a href = "/{{item._id}}/{{item.slug}}">
      {%if item.cover.url %}
      <img src="{{item.cover.url}}"  alt="smaple image" class = "cover"/>
      {%else%}
      <div class= "cover">
      <img src="/images/controller.svg"  alt="search logo" class = "no-cover-logo"/>
      </div>
      {%endif%}
      </a>
      <p> <a href = "/{{item._id}}/{{item.slug}}" class="link-light"> {{item.name}} </a></p>
    </div>
    {% endif %}
  {%endfor%}
  </div>
</div>



